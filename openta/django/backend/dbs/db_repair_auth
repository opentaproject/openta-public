#!/bin/bash
for i in /subdomain-data/$1
do
	subdomain=`basename $i`
	echo $subdomain
	dbnamefile="/subdomain-data/${subdomain}/dbname.txt"
	echo $dbnamefile
    	if test -e $dbnamefile ; then
    		echo "FILE ${dbnamefile} FOUND"
        else
    		echo "No database found"
		exit
	fi
	SUBDOMAIN=$subdomain python manage.py  repair_auth --list=false --settings=backend.settings_manage $2
	#SUBDOMAIN=$subdomain ./db_backup $subdomain
done
touch /subdomain-data/last_reset
