# .coveragerc to control coverage.py
[run]
branch = True
omit = ./venv/*,*test*,*apps.py,*manage.py,*__init__.py,*migrations*,*asgi*,*wsgi*,*admin.py,*urls.py

[report]
# Regexes for lines to exclude from consideration
omit = ./venv/*,*test*,*apps.py,*manage.py,*__init__.py,*migrations*,*asgi*,*wsgi*,*admin.py,*urls.py

[html]
directory = coverage_html_report
